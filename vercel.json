{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|svg|png|jpg|jpeg|gif|ico|woff|woff2|webp|avif)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/report-schedule",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/report-execution",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/sync-transactions",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/sync-terminals",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/sync-settlements",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/sync-payouts",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/sync-merchants",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/sync-paymentLinks",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/test-notification",
      "schedule": "0 9 28-31 * *"
    }
  ]
}
